<script>
  const track = document.querySelector('.skills-track');
  if (track) {
    const originals = Array.from(track.children);
    originals.slice().reverse().forEach(item => {
      const clone = item.cloneNode(true);
      track.insertBefore(clone, track.firstChild);
    });

    const cycleWidth = track.scrollWidth / 2;

    const styleEl = document.createElement('style');
    styleEl.textContent = `
      @keyframes scrollRev {
        from { transform: translateX(-${cycleWidth}px); }
        to   { transform: translateX(0); }
      }
    `;
    document.head.append(styleEl);

    (track as HTMLElement).style.animation = 'scrollRev 20s linear infinite';
  }

  const track3 = document.querySelector('#track3');
    if (track3) {
    const originals = Array.from(track3.children);
    originals.slice().reverse().forEach(item => {
      const clone = item.cloneNode(true);
      track3.insertBefore(clone, track3.firstChild);
    });

    const cycleWidth = track3.scrollWidth / 2;

    const styleEl = document.createElement('style');
    styleEl.textContent = `
      @keyframes scrollRev {
        from { transform: translateX(-${cycleWidth}px); }
        to   { transform: translateX(0); }
      }
    `;
    document.head.append(styleEl);

    (track3 as HTMLElement).style.animation = 'scrollRev 20s linear infinite';
  }
  
  function duplicateContent(trackId: string) {
    const track = document.getElementById(trackId);
    if (!track) return;
      const children = Array.from(track.children);
      children.forEach(child => {
        const clone = child.cloneNode(true);
        track.appendChild(clone);
      });
    }
        
    duplicateContent('track');
    duplicateContent('track2');
    duplicateContent('track3');
</script>

<style is:global>
  .track-container {
    position: relative;
    overflow: hidden;
    padding: 1rem 0;
  }
      
  .track-container::before,
  .track-container::after {
    content: '';
    position: absolute;
    top: 0;
    bottom: 0;
    width: 100px;
    z-index: 2;
  }
        
  .skills-track {
    display: flex;
    width: max-content;
    animation: scroll 40s linear infinite;
  }
  @keyframes scroll {
    0% { transform: translateX(0); }
    100% { transform: translateX(-50%); }
  }
</style>

