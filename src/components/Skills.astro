<script>
  const track = document.querySelector('.skills-track');
  if (track) {
    const originals = Array.from(track.children);
    originals.slice().reverse().forEach(item => {
      const clone = item.cloneNode(true);
      track.insertBefore(clone, track.firstChild);
    });

    const cycleWidth = track.scrollWidth / 2;

    const styleEl = document.createElement('style');
    styleEl.textContent = `
      @keyframes scrollRev {
        from { transform: translateX(-${cycleWidth}px); }
        to   { transform: translateX(0); }
      }
    `;
    document.head.append(styleEl);

    (track as HTMLElement).style.animation = 'scrollRev 10s linear infinite';
  }

  const track2 = document.querySelector('.skills-track2');
  if (track2) {
    const originals = Array.from(track2.children);
    originals.slice().reverse().forEach(item => {
      const clone = item.cloneNode(true);
      track2.insertBefore(clone, track2.firstChild);
    });
    const cycleWidth = track2.scrollWidth / 2;
      
    const styleEl = document.createElement('style');
    styleEl.textContent =
        `@keyframes scrollCycle {
          from { transform: translateX(0); }
          to   { transform: translateX(-${cycleWidth}px); }
        }`;
      document.head.append(styleEl);

      (track2 as HTMLElement).style.animation = 'scrollCycle 10s linear infinite';
    }
</script>

